<%- model_class = Proveedor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:RFC) %>:</strong></dt>
  <dd><%= @proveedor.RFC %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Nombre) %>:</strong></dt>
  <dd><%= @proveedor.Nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Direccion) %>:</strong></dt>
  <dd><%= @proveedor.Direccion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Telefono) %>:</strong></dt>
  <dd><%= @proveedor.Telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Producto) %>:</strong></dt>
  <dd><%= @proveedor.Producto %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              proveedores_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_proveedor_path(@proveedor), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              proveedor_path(@proveedor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
							:class => 'btn btn-danger' %>
<br/><br/>

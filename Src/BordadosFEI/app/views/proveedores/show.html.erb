<div class="panel panel-default">
  <div class="panel-heading"><h1>Productos</h1></div>
  <div class="panel-body">
<p id="notice"><%= notice %></p>

<p>
  <strong>Rfc:</strong>
  <%= @proveedor.RFC %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @proveedor.Nombre %>
</p>

<p>
  <strong>Direccion:</strong>
  <%= @proveedor.Direccion %>
</p>

<p>
  <strong>Telefono:</strong>
  <%= @proveedor.Telefono %>
</p>
</div>
<div class="panel-footer">
<%= link_to 'Editar', edit_proveedor_path(@proveedor), class: "btn btn-primary" %>
<%= link_to 'Regresar', proveedores_path, class: "btn btn-default" %>
</div>
